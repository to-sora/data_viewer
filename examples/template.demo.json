{
  "ordering": [
    "caption\\d+_txt",
    "OCR_json",
    "OCR_txt"
  ],
  "annotations": {
    "caption\\d+_txt": {
      "readonly": false
    },
    "OCR_json": {
      "readonly": true,
      "functions": [
        {"name": "first 10 characters", "filter": "data['result'][0]['text'][:10]"}
      ]
    },
    "OCR_txt": {
      "readonly": true,
      "functions": [
        {"name": "Character name", "filter": "Character name :"}
      ]
    }
  }
}
